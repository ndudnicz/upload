<p><%= data['filename'] %></p>
<% if (data['size'] < 1000) { %>
	<p><%= data['size'] %> B</p>
<% }
else if (data['size'] < 1000000) { %>
	<p><%= parseInt(data['size'] / 1000) %> KB</p>
<% }
else { %>
	<p><%= (data['size'] / 1000000).toString().substr(0, 4) %> MB</p>
<% } %>
<p>Available until: <%= new Date(data['deleted']).toLocaleDateString() %> <%= new Date(data['deleted']).toLocaleTimeString() %></p>
<a href="/download/<%= data['path'] %>">Download</a>
<a href="/report/<%= data['path'] %>">Report</a>
